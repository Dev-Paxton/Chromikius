{
    "compilerOptions": {
        "module": "CommonJS",
        "moduleResolution": "node",
        "target": "ESNext",
        "outDir": "dist",
        "esModuleInterop": true,
        "allowSyntheticDefaultImports": true,
        "resolveJsonModule": true
    },
    "include": ["main.ts", "deploy_commands.ts", "src", "src/**/*.json", "config/**/*.json"],
    "exclude": ["node_modules"]
}